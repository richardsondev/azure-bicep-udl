<?xml version="1.0" encoding="UTF-8"?>
<NotepadPlus>
    <UserLang name="Bicep" ext="bicep" udlVersion="2.1">
        <Settings>
            <Global caseIgnored="no" allowFoldOfComments="1" foldCompact="1" forcePureLC="0" decimalSeparator="." />
            <Prefix Keywords1="yes" Keywords2="yes" Keywords3="yes" Keywords4="yes" Keywords5="yes" Keywords6="yes" Keywords7="yes" Keywords8="yes"/>
        </Settings>
        <KeywordLists>
            <Keywords name="Comments">// /* */</Keywords>
            <Keywords name="Numbers, prefix1">+ -</Keywords>
            <Keywords name="Numbers, prefix2"></Keywords>
            <Keywords name="Numbers, extras1">.</Keywords>
            <Keywords name="Numbers, extras2">e E</Keywords>
            <Keywords name="Numbers, suffix1">f F d D</Keywords>
            <Keywords name="Numbers, suffix2"></Keywords>
            <Keywords name="Numbers, range"></Keywords>
            <Keywords name="Operators1">= + - * / % &amp; | ^ ! ~ &lt; &gt; &lt;= &gt;= == != ?? : ?</Keywords>
            <Keywords name="Operators2"></Keywords>
            <Keywords name="Folders in code1, open">{ [ (</Keywords>
            <Keywords name="Folders in code1, middle"></Keywords>
            <Keywords name="Folders in code1, close">} ] )</Keywords>
            <Keywords name="Folders in code2, open"></Keywords>
            <Keywords name="Folders in code2, middle"></Keywords>
            <Keywords name="Folders in code2, close"></Keywords>
            <Keywords name="Folders in comment, open"></Keywords>
            <Keywords name="Folders in comment, middle"></Keywords>
            <Keywords name="Folders in comment, close"></Keywords>
            
            <!-- Bicep Keywords -->
            <Keywords name="Keywords1">metadata targetScope resource module param var output for in if existing import as type with using extends func assert extension</Keywords>
            <!-- Named Literals -->
            <Keywords name="Keywords2">true false null</Keywords>
            <!-- Logical Operators -->
            <Keywords name="Keywords3">and or not</Keywords>
            <Keywords name="Keywords4">any</Keywords>
            <Keywords name="Keywords5">#disable-next-line</Keywords>
            <Keywords name="Keywords6"></Keywords>
            <Keywords name="Keywords7"></Keywords>
            <Keywords name="Keywords8"></Keywords>
        </KeywordLists>
        <Styles>
            <WordsStyle name="DEFAULT" styleID="0" fgColor="FFFFFF" bgColor="000000" fontStyle="0" nesting="0" />
            <WordsStyle name="COMMENTS" styleID="1" fgColor="FFFFFF" bgColor="008000" fontStyle="0" nesting="0" />
            <WordsStyle name="LINE COMMENTS" styleID="2" fgColor="FFFFFF" bgColor="008000" fontStyle="0" nesting="0" />
            <WordsStyle name="NUMBERS" styleID="4" fgColor="FFFFFF" bgColor="FF8000" fontStyle="0" nesting="0" />
            <WordsStyle name="KEYWORDS1" styleID="5" fgColor="FFFFFF" bgColor="0000FF" fontStyle="1" nesting="0" />
            <WordsStyle name="KEYWORDS2" styleID="6" fgColor="FFFFFF" bgColor="800080" fontStyle="1" nesting="0" />
            <!-- Make Keywords3 stand out (red and bold): -->
            <WordsStyle name="KEYWORDS3" styleID="7" fgColor="FFFFFF" bgColor="FF0000" fontStyle="1" nesting="0" />
            <!-- Keywords4 (e.g., 'any') normal style -->
            <WordsStyle name="KEYWORDS4" styleID="8" fgColor="FFFFFF" bgColor="000000" fontStyle="0" nesting="0" />
            <WordsStyle name="KEYWORDS5" styleID="9" fgColor="FFFFFF" bgColor="000000" fontStyle="0" nesting="0" />
            <WordsStyle name="KEYWORDS6" styleID="12" fgColor="FFFFFF" bgColor="000000" fontStyle="0" nesting="0" />
            <WordsStyle name="KEYWORDS7" styleID="13" fgColor="FFFFFF" bgColor="000000" fontStyle="0" nesting="0" />
            <WordsStyle name="KEYWORDS8" styleID="14" fgColor="FFFFFF" bgColor="000000" fontStyle="0" nesting="0" />
            <WordsStyle name="OPERATORS" styleID="10" fgColor="FF0000" bgColor="000000" fontStyle="0" nesting="0" />
            <WordsStyle name="FOLDER IN CODE1" styleID="11" fgColor="FFFFFF" bgColor="800000" fontStyle="0" nesting="0" />
            <WordsStyle name="DELIMITERS1" styleID="16" fgColor="FFFFFF" bgColor="008000" fontStyle="0" nesting="0" />
        </Styles>

        <Comment>
            <WordsStyle name="COMMENT" fgColor="FFFFFF" bgColor="008000" fontStyle="0"/>
            <WordsStyle name="COMMENT LINE" fgColor="FFFFFF" bgColor="008000" fontStyle="0"/>
            <WordsStyle name="COMMENT DOC" fgColor="FFFFFF" bgColor="008000" fontStyle="0"/>
        </Comment>

        <CommentStrings>
            <CommentString type="LINE" value="//" />
            <CommentString type="BLOCK" value="/*" value2="*/" />
        </CommentStrings>

        <Delimiter open="'" close="'" escape="" lineFold="no">
            <WordsStyle name="DELIMITERS1" fgColor="FFFFFF" bgColor="008000" fontStyle="0" nesting="0"/>
        </Delimiter>

        <FolderMaps>
            <FolderMap styleID="FOLDER IN CODE1" open="{" close="}"/>
            <FolderMap styleID="FOLDER IN CODE1" open="[" close="]"/>
            <FolderMap styleID="FOLDER IN CODE1" open="(" close=")"/>
        </FolderMaps>

        <Numbers>
            <WordsStyle name="NUMBERS" fgColor="FFFFFF" bgColor="FF8000" fontStyle="0"/>
        </Numbers>
    </UserLang>
</NotepadPlus>
